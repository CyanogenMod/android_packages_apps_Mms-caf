<resources>
    <!-- String to match as no subject and filter out as a subject. For example, if the
         subject string is "no subject", we won't display that. We'll pretend
         no subject string was delivered. -->
    <string-array name="empty_subject_strings">
        <item>no subject</item>
        <item>NoSubject</item>
    </string-array>

    <string-array name="box_type">
        <item>@string/type_inbox</item>
        <item>@string/type_sent</item>
        <item>@string/type_draft</item>
        <item>@string/type_outbox</item>
    </string-array>
    <string-array name="slot_type">
        <item>@string/type_all</item>
        <item>@string/type_slot1</item>
        <item>@string/type_slot2</item>
    </string-array>

    <string-array name="save_time">
        <item>@string/mms_max</item>
        <item>@string/mms_two_days</item>
    </string-array>

    <string-array name="save_time_values">
        <item>0</item>  <!-- maxmum -->
        <item>172800</item>   <!--2*24*60*60 -->
    </string-array>

    <string-array name="number_options">
        <item>@string/menu_call</item>
        <item>@string/title_send_message</item>
        <item>@string/menu_add_to_contacts</item>
    </string-array>

    <string-array name="entries_sms_store_value">
        <item>0</item>
        <item>1</item>
    </string-array>

    <string-array name="entries_sms_store">
        <item>@string/pref_sms_store_phone</item>
        <item>@string/pref_sms_store_card</item>
    </string-array>

    <string-array name="entries_cdma_priority" translatable="false">
        <item>@string/cdma_priority_normal</item>
        <item>@string/cdma_priority_interactive</item>
        <item>@string/cdma_priority_urgent</item>
        <item>@string/cdma_priority_emergency</item>
    </string-array>

    <string-array name="values_cdma_priority" translatable="false">
        <item>0</item>
        <item>1</item>
        <item>2</item>
        <item>3</item>
    </string-array>

    <string-array name="entries_list_search_type">
        <item>@string/search_as_content</item>
        <item>@string/search_as_name</item>
        <item>@string/search_as_number</item>
        <item>@string/search_as_subject</item>
