<resources>
    <!-- configuration for filtering illegal character in address -->
    <bool name="config_filter_char_address">false</bool>

    <!-- configuration for retry immediately when send MMS failed -->
    <bool name="config_retry_immediately">false</bool>

    <!-- configuration for reconnect data call immediately -->
    <bool name="config_reconnect">false</bool>

    <!-- configuration for prompt dialog for wap push link -->
    <bool name="config_wap_push">false</bool>

    <!-- configuration for show two call button in conversation -->
    <bool name="config_two_call_button">true</bool>

    <!-- configuration for max slide num -->
    <integer name="max_slide_num">10</integer>
</resources>
